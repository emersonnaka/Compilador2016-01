flutuante somar (flutuante: a, flutuante: b)
	retorna(a + b)
fim

flutuante subtrair (flutuante: a, flutuante: b)
	retorna(a - b)
fim

flutuante multiplicar (flutuante: a, flutuante: b)
	retorna(a * b)
fim

flutuante dividir (flutuante: a, flutuante: b)
	retorna(a / b)
fim

flutuante teste()
	se 1 = 1 então
		retorna(dividir(2, 3))
	senão
		retorna(multiplicar(2, 2))
	fim
fim

vazio principal()
	flutuante: r
	r := somar(3, 5)
	r := subtrair(3, 5)
	r := multiplicar(3, 5)
	r := dividir(3, 5)
fim
